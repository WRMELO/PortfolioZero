        "artifacts": [
          "planning/runs/TASK_A_003/S2_RUN_AGNO_PROBE.txt"
        ]
      },
      {
        "step_id": "S3_SHOW_HELP_SNIPPETS",
        "type": "shell",
        "commands": [
          "cd /home/wilson/PortfolioZero && (command -v agno && agno --help | head -n 120) || true",
          "cd /home/wilson/PortfolioZero && (python -m agno --help | head -n 120) || true"
